{
    "name": "retinens/laravel-dashboard-ploi-tile",
    "description": ":package_description",
    "keywords": [
        "retinens",
        "laravel-dashboard-ploi-tile",
        "ploi-tile",
        "ploi.io"
    ],
    "homepage": "https://github.com/retinens/laravel-dashboard-ploi-tile",
    "license": "MIT",
    "authors": [
        {
            "name": "Lucas Houssa",
            "email": "lucas@retinens.com",
            "homepage": "https://retinens.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0|^7.4",
        "ploi/ploi-php-sdk": "^1.8",
        "spatie/laravel-dashboard": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Retinens\\PloiTile\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Retinens\\PloiTile\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Retinens\\PloiTile\\PloiTileServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
